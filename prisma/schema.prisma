generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model Users {
  id              String    @id @default(auto()) @map("_id") @db.ObjectId
  NOMBRE_COMPLETO String
  DNI             Int
  COMUNIDAD       String?
  CDG_COMUNIDAD   Int?
  CODIGO          Int?
  CLAVE           String
  EMAIL           String?
  URL_IMAGE       String?
  FECHA_REGISTRO  DateTime?
  ROL             Roles     @default(ARTESANO)
}

enum Roles {
  ARTESANO
  ADMIN
}
