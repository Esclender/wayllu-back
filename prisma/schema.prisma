generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model Artisans {
  id              String   @id @default(auto()) @map("_id") @db.ObjectId
  NOMBRE_COMPLETO String
  DNI             Int
  COMUNIDAD       String
  CDG_COMUNIDAD   Int
  CODIGO          Int      @unique
  CLAVE           String
  EMAIL           String?
  URL_IMAGE       String?
  FECHA_REGISTRO  DateTime
}
